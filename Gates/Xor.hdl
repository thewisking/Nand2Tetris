/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in = a, out = Na);
    Not(in = b, out = Nb);

    And(a = Na, b = b, out = Nab);
    And(a = Nb, b = a, out = Nba);

    Or(a = Nab, b = Nba, out = out);



}